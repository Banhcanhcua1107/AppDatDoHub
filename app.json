{
  "expo": {
    "name": "TableFlow",
    "slug": "tableflow-restaurant",
    "version": "1.0.0",
    "scheme": "exp+tableflow-restaurant",
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-font",
      "expo-asset"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bephubbar.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bephubbar.app"
    },
    "extra": {
      "eas": {
        "projectId": "7a061be6-293b-4b64-8ae8-b7f2f576a0fe"
      }
    }
  }
}
